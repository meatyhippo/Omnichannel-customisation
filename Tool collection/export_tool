javascript:!function(){var e = new XMLHttpRequest();e.open("GET", "https://api.github.com/repos/meatyhippo/Omnichannel-customisation/releases", true),e.onload = function(){if ( e.status >= 200 && e.status < 400 ){let vers = JSON.parse(e.responseText);let version_ = vers[0].tag_name;console.log("git version "+vers[0].tag_name);GIT_(version_);}},e.send();}();
function GIT_(version_){console.log("backoffice");var script_ = document.createElement('script');script_.src = `https://cdn.jsdelivr.net/gh/meatyhippo/Omnichannel-customisation@${version_}/Scripts/Retail/Export_runner.js`;document.body.appendChild(script_);};
