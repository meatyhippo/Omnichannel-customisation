Copy the Javascript line below in the url field of a bookmark
-------------------

javascript: !function(){var e=new XMLHttpRequest;e.open("GET",location.origin+"/whois.json",true),e.onload=function(){if(e.status>=200&&e.status<400){var t=JSON.parse(e.responseText);console.log(t);console.log(t.resellerId);console.log();if(t.clusterId==="eu1"){switch(t.resellerId){case 1:window.alert("The shop is on the Dutch cluster");break;case 14:window.alert("The shop is on the German cluster");break;case 15:window.alert("The shop is on the Spanish cluster");break;case 19:window.alert("The shop is on the Russian cluster");break;case 23:window.alert("The shop is on the Norwegian cluster");break;case 28:window.alert("The shop is on the UK cluster");break;case 38:window.alert("The shop is on the US cluster");break;case 40:window.alert("The shop is on the CAN cluster");break;case 42:window.alert("The shop is on the ROW cluster");break;case 44:window.alert("The shop is on the REMEA cluster");default:window.alert("ERROR")}}else if(t.clusterId==="us1"){switch(t.resellerId){case 1:window.alert("The shop is on the SEOshop cluster");break;case 1e3:window.alert("The shop is on the Lightspeed USA cluster");break;case 1001:window.alert("The shop is on the Lightspeed Canada cluster");break;case 1002:window.alert("The shop is on the Lightspeed Rest of World cluster");break;case 1003:window.alert("The shop is on the Lightspeed Australia cluster");break;default:window.alert("ERROR")}}}},e.send()}();
