javascript:!function(){var e=new XMLHttpRequest();e.open("GET","https://api.github.com/repos/rubenvanhee/shop_info_css/releases",!0),e.onload=function(){if(e.status>=200&&e.status<400){var version=JSON.parse(e.responseText);let version_=version[0].tag_name;console.log("git version"+version[0].tag_name);GIT_(version_)}},e.send()}();function GIT_(version_){if(location.pathname.includes("/admin/")){console.log("backoffice");var script_=document.createElement('script');script_.src=`https://cdn.jsdelivr.net/gh/rubenvanhee/shop_info_css@${version_}/beta_version_backoffice.js`;document.body.appendChild(script_);var styling=document.createElement('link');styling.rel="stylesheet",styling.href=`https://cdn.jsdelivr.net/gh/rubenvanhee/shop_info_css@${version_}/info_style.css`,document.head.appendChild(styling)}else{console.log("frontoffice");var script_=document.createElement('script');script_.src=`https://cdn.jsdelivr.net/gh/rubenvanhee/shop_info_css@${version_}/beta_version.js`;document.body.appendChild(script_);var styling=document.createElement('link');styling.rel="stylesheet",styling.href=`https://cdn.jsdelivr.net/gh/rubenvanhee/shop_info_css@${version_}/info_style.css`,document.head.appendChild(styling)}}
